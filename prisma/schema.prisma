generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Post {
  id          String    @id @default(uuid())
  content     String
  scheduledAt DateTime
  postedAt    DateTime?
  failedAt    DateTime?
  error       String?
  createdAt   DateTime  @default(now())
}
